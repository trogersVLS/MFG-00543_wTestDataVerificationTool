<?xml version="1.0"?>
<!-- For more information on using app.config transformation visit http://go.microsoft.com/fwlink/?LinkId=125889 -->
<!-- In case configuration is not the root element, replace it with root element in source configuration file -->
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <appSettings>
    <add key ="Location" value="Bothell" xdt:Locator="Match(key)" xdt:Transform="InsertIfMissing" />
    <add key ="Environment" value ="Production" xdt:Locator="Match(key)" xdt:Transform="InsertIfMissing" />
    <add key ="ConfigPath" value ="Settings.xml" xdt:Locator="Match(key)" xdt:Transform="InsertIfMissing" />
  </appSettings>
</configuration>